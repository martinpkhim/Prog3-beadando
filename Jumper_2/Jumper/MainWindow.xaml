<Window x:Name="MWindow" x:Class="Jumper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jumper"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" SizeChanged="MWindow_SizeChanged" ResizeMode="NoResize" KeyDown="MWindow_KeyDown" >
    <Window.Resources>
        <Style TargetType="Button">
            <Setter Property="Width" Value="80"/>
            <Setter Property="Height" Value="25"/>
        </Style>
    </Window.Resources>
    <Grid>
        <Canvas x:Name="CanvasBackground" HorizontalAlignment="Left" Height="320" VerticalAlignment="Top" Width="517"/>
        

    </Grid>
</Window>
